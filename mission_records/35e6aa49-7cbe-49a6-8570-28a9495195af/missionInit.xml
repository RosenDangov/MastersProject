<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<MissionInit xmlns="http://ProjectMalmo.microsoft.com" PlatformVersion="0.30.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ProjectMalmo.microsoft.com MissionInit.xsd">
  <Mission>
    <About>
      <Summary>Cliff walking mission based on Sutton and Barto.</Summary>
    </About>
    <ModSettings>
      <MsPerTick>1</MsPerTick>
    </ModSettings>
    <ServerSection>
      <ServerInitialConditions>
        <Time>
          <StartTime>6000</StartTime>
          <AllowPassageOfTime>false</AllowPassageOfTime>
        </Time>
        <Weather>clear</Weather>
        <AllowSpawning>false</AllowSpawning>
      </ServerInitialConditions>
      <ServerHandlers>
        <FlatWorldGenerator destroyAfterUse="true" forceReset="false" generatorString="3;7,220*1,5*3,2;3;,biome_1" seed=""/>
        <DrawingDecorator>
          <DrawCuboid type="air" x1="-2" x2="7" y1="46" y2="50" z1="-2" z2="18"/>
          <DrawCuboid type="lava" x1="-2" x2="7" y1="45" y2="45" z1="-2" z2="18"/>
          <DrawCuboid type="sandstone" x1="1" x2="3" y1="45" y2="45" z1="1" z2="12"/>
          <DrawBlock type="cobblestone" x="4" y="45" z="1"/>
          <DrawBlock type="lapis_block" x="4" y="45" z="12"/>
          <DrawItem type="diamond" x="4" y="46" z="12"/>
        </DrawingDecorator>
        <DrawingDecorator>
          <DrawBlock type="lava" x="2" y="45" z="2"/>
        </DrawingDecorator>
        <DrawingDecorator>
          <DrawBlock type="lava" x="3" y="45" z="4"/>
        </DrawingDecorator>
        <DrawingDecorator>
          <DrawBlock type="lava" x="2" y="45" z="6"/>
        </DrawingDecorator>
        <DrawingDecorator>
          <DrawBlock type="lava" x="2" y="45" z="8"/>
        </DrawingDecorator>
        <DrawingDecorator>
          <DrawBlock type="lava" x="3" y="45" z="10"/>
        </DrawingDecorator>
        <ServerQuitFromTimeUp description="" timeLimitMs="1000000"/>
        <ServerQuitWhenAnyAgentFinishes description=""/>
      </ServerHandlers>
    </ServerSection>
    <AgentSection mode="Survival">
      <Name>Cristina</Name>
      <AgentStart>
        <Placement pitch="30" x="4.5" y="46" yaw="0" z="1.5"/>
      </AgentStart>
      <AgentHandlers>
        <ObservationFromFullStats/>
        <VideoProducer viewpoint="1" want_depth="false">
          <Width>320</Width>
          <Height>240</Height>
        </VideoProducer>
        <RewardForTouchingBlockType dimension="0">
          <Block behaviour="onceOnly" cooldownInMs="1000" distribution="" reward="-100" type="lava"/>
          <Block behaviour="onceOnly" cooldownInMs="1000" distribution="" reward="100" type="lapis_block"/>
        </RewardForTouchingBlockType>
        <RewardForSendingCommand dimension="0" distribution="" reward="-1"/>
        <DiscreteMovementCommands autoFall="false" autoJump="false"/>
        <AgentQuitFromTouchingBlockType>
          <Block description="" type="lava"/>
          <Block description="" type="lapis_block"/>
        </AgentQuitFromTouchingBlockType>
      </AgentHandlers>
    </AgentSection>
  </Mission>
  <ExperimentUID>tabular_q_learning-1</ExperimentUID>
  <ClientRole>0</ClientRole>
  <ClientAgentConnection>
    <ClientIPAddress>127.0.0.1</ClientIPAddress>
    <ClientMissionControlPort>10000</ClientMissionControlPort>
    <ClientCommandsPort>0</ClientCommandsPort>
    <AgentIPAddress>127.0.0.1</AgentIPAddress>
    <AgentMissionControlPort>10268</AgentMissionControlPort>
    <AgentVideoPort>10881</AgentVideoPort>
    <AgentObservationsPort>10060</AgentObservationsPort>
    <AgentRewardsPort>10892</AgentRewardsPort>
  </ClientAgentConnection>
</MissionInit>
